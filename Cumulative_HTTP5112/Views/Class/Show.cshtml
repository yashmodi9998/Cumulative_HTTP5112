@model Cumulative_HTTP5112.Models.Class
@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Class/List">Back To Classes</a>
<div id="containershow">
    <h2>Class: @Model.ClassName  </h2>
    <p>Class Id: @Model.ClassId </p>
    <p> Class code: @Model.ClassCode</p>
    <p> Teacher Id: @Model.TeacherId</p>
    <p> Duration: @Model.StartDate to @Model.FinishDate</p>
</div>

